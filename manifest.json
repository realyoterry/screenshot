{
    "manifest_version": 3,
    "name": "Screenshot",
    "version": "1.0.0",
    "description": "Screenshot full screen, or selected area.",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "downloads",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}